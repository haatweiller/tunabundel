\begin{song}{La Bikina}
\rhythmandkey[\nicefrac{6}{8}]{Joropo}{G}
het is geen 6/8 lololol\\
\begin{instrumental}{Intro}

\measure{G} \measure{} \measure{B7} \measure{} \measure{Em} \measure{} \measure{Dm} \measure{G7} \measure{C} \measure{B7} \measure{Em} \measure{Cm$\downarrow$} \measure{G} \measure{D} \measure{G} \measure*{G$\downarrow$}
\end{instrumental}

\begin{verse}{Couplet 1}

Soli\chord{G}taria camina la bi\chord{B7}kina,\\
y la \chord{Em}gente se pone a murmu\chord{Dm}rar,\hspace{1em}\chord{G7}\\
\chord{C}dicen que \chord{B7}tiene una \chord{Em}pena,\\
\chord{A7}dicen que tiene una pena que le hace llo\chord{D}rar. \chord{D7$\downarrow$}
\end{verse}

\begin{verse}{Couplet 2}

Alta\chord{G}nera, preciosa y orgu\chord{B7}llosa,\\
no per\chord{Em}mite la quieran conso\chord{Dm}lar, \hspace{1em}\chord{G7}\\
\chord{C}pasa lu\chord{B7}ciendo su \chord{Em}real maje\chord{Cm$\downarrow$}stad\\
\chord{G}pasa, camina, los \chord{D}mira sin verlos ja\chord{G}más.\\
\end{verse}

